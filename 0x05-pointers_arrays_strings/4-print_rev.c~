#include "main.h"

/**
 * _puts - prints a string followed by newline
 * @str: character array to be printed
 */

int astrlen(char *s);
void print_rev(char *s)
{
  int j;

 for (j = astrlen(s); *(s + j) >= 0; j--)
	{char k = *(s + j);
		_putchar(k);
	}
	_putchar('\n');
}

int astrlen(char *s)
{
  int i, res = 0;
  for (i = 0; *(s + 1); i++)
    res += 1;
  _putchar(res + '0');
  return res;
}
